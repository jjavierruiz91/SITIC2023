
@{
    ViewBag.Title = "Agregar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script defer src="~/Scripts/APP/Medicos/Medicos.js"></script>
<h2>Resgitro Especialistas</h2>

<hr width=100% align="right" size=200 color="black">

<div class="row m-0">

    <form id="FormMedicosDeportiva" name="FormMedicosDeportiva">

        <div class="row m-0">
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Numero Identificación</label>
                <input class="form-control" id="CodMedicos" name="CodMedicos" />
            </div>
            
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Primer Nombre</label>
                <input class="form-control" id="PrimerNombre" name="PrimerNombre" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Segundo Nombre</label>
                <input class="form-control" id="SegundoNombre" name="SegundoNombreSegundoNombre" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Primer Apellido</label>
                <input class="form-control" id="PrimerApellido" name="PrimerApellido" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Segundo Apellido</label>
                <input class="form-control" id="SegundoApellido" name="SegundoApellido" />
            </div>
          
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Fecha Nacimiento</label>
                <input type="date" class="form-control" id="FechaNacimiento" name="FechaNacimiento" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Especialidad</label>
                <input class="form-control" id="Especialidad" name="Especialidad" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Direcccion</label>
                <input class="form-control" id="Direcccion" name="Direcccion" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label> Telefono  </label>
                <input class="form-control" id="Telefono" onkeypress="return soloNumeros(event)" name="Telefono" />
            </div>

            <div class="col-xs-4 col-l-4 col-md-4">
                <label>Centro</label>
                <input type="text" class="form-control" id="Centro" name="Centro" />
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Genero</label>
                <select class="select2 form-control" id="Genero" name="Genero">
                    <option value="">Seleccionar</option>
                    <option value="MASCULINO">MASCULINO</option>
                    <option value="FEMENINO">FEMENINO</option>
                </select>
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4">
                <label>Correo</label>
                <input type="text" class="form-control" id="Correo" name="Correo" />
            </div>
           
        </div>
    </form>

    <div class="col-xs-4 col-sm-4 col-md-4">
        <button type="button" onclick="Createobj()" class="btn btn-block btn-outline-primary btn-rounded btn-outline mt-4" id="SaveMedicos">Guardar</button>
    </div>
</div>




