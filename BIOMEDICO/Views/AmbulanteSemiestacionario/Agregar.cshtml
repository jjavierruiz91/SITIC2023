<script>
    function validarCedulaDuplicada() {
        var cedula = $('#NumeroIDAmbulanteSemiestacionario').val();
        var modalMessageParagraph = $('#modalMessage');
        modalMessageParagraph.text('');
        modalMessageParagraph.css('color', 'black');

        if (cedula === "") {
            modalMessageParagraph.text('El número de identificación es requerido.');
            modalMessageParagraph.css('color', 'red');
            $('#validationModal').modal('show');
            return;
        }

        if (isNaN(parseInt(cedula))) {
            modalMessageParagraph.text('Por favor, ingresa solo números en el campo de identificación.');
            modalMessageParagraph.css('color', 'red');
            $('#validationModal').modal('show');
            return;
        }

        $.ajax({
            url: '@Url.Action("ValidarCedula", "AmbulanteSemiestacionario")',
            type: 'GET',
            data: { cedula: cedula },
            success: function(response) {
                if (response.Error) {
                    // Solo mostrar alerta si HAY ERROR (cédula duplicada)
                    modalMessageParagraph.text(response.mensaje);
                    modalMessageParagraph.css('color', 'red');
                    $('#validationModal').modal('show');
                }
                // Si no hay error (cédula disponible), no hacer nada - sin alerta
            },
            error: function(xhr, status, error) {
                console.error("Error en la validación AJAX: ", error);
                modalMessageParagraph.text('Ocurrió un error inesperado al verificar el número de identificación. Por favor, intenta de nuevo.');
                modalMessageParagraph.css('color', 'red');
                $('#validationModal').modal('show');
            }
        });
    }

    function limpiarMensajeCedula() {
        $('#validationModal').modal('hide');
        $('#modalMessage').text('');
        $('#modalMessage').css('color', 'black');
    }
</script>